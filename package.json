{
  "name": "blogger",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "16 || 18"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.0"
  },
  "scripts": {
    "cy": "cypress run",
    "cy:open": "cypress open --e2e --browser chrome",
    "build": "vite build",
    "test": "vitest",
    "lint:styles": "npx stylelint '**/*.css'",
    "lint": "eslint --no-inline-config --no-error-on-unmatched-pattern -c .eslintrc.json . --ext .js,.jsx,.ts,.tsx",
    "dev": "vite --open",
    "start": "vite",
    "preview": "vite preview",
    "coverage": "vitest run --coverage --watch=false",
    "cy-coverage": "cross-env NODE_OPTIONS=- vitest run --coverage --watch=false"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/dom": "^8.18.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@trybe/eslint-config-frontend": "^1.11.3",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "@vitest/coverage-c8": "^0.30.1",
    "cypress": "^12.4.0",
    "cypress-multi-reporters": "^1.6.1",
    "fs-extra": "^10.1.0",
    "jsdom": "^21.1.0",
    "mocha": "^10.0.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0",
    "stylelint": "^14.13.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-order": "5.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.1.4",
    "vitest": "^0.30.1"
  }
}
